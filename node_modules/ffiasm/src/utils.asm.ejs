<% global.setTypeDest = function (t) {
return (
`        mov r11b, ${t}
        shl r11d, 24
        mov [rdi+4], r11d`);
} %>


<% global.toMont_a = function () {
return (
`
        mov  r8, rdi
        sub  rsp, ${ (n64+1)*8 }
        mov  rdi, rsp
        push rdx
        push r8
        call ${name}_toMontgomery
        mov  rsi, rdi
        pop  rdi
        pop  rdx
`);
} %>

